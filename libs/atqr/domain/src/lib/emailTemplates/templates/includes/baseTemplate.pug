
mixin emailContent(title, p1, imgSrc, p2, p3, p4)
  .content-frame
    .content-wrapper
      h1=title
      p.p1=p1
      |
      .image
        img(src=imgSrc)
      p.p2=p2
      |
      p.p3=p3
      |
      p.p4=p4
      |

mixin button(href, name)
  .btn-wrapper
    a(href=href)&attributes(attributes)= name


