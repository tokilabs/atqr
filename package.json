{
  "name": "atqr",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "_________ ATQR DEV_________": "_________________",
    "atqr:start": "concurrently \"yarn atqr:start:web\" \"yarn atqr:start:api\" ",
    "preatqr:start:api": "docker compose up -d",
    "atqr:start:api": "nx serve api",
    "postatqr:start:api": "docker compose down",
    "atqr:start:web": "yarn parcel apps/atqr/web/src/pages/**/*.pug --open",
    "_________ATQR PROD_________": "_________________",
    "atqr:build:web": "yarn parcel build apps/atqr/web/src/pages/**/*.pug",
    "_________ATQR DB_________": "_________________",
    "atqr:db:migrate": "yarn prisma migrate  --schema ./apps/atqr/api/prisma/schema.prisma",
    "atqr:db:dev": "yarn prisma migrate dev --schema ./apps/atqr/api/prisma/schema.prisma",
    "atqr:db:format": "yarn prisma format --schema ./apps/atqr/api/prisma/schema.prisma",
    "atqr:db:generate": "yarn prisma generate --schema ./apps/atqr/api/prisma/schema.prisma",
    "atqr:db:push": "yarn prisma db push --schema ./apps/atqr/api/prisma/schema.prisma",
    "_________ATQR REPO MAINTENANCE_________": "_________________",
    "prepare": "husky install",
    "post-checkout": "yarn && yarn run atqr:db:generate",
    "commit": "git-cz",
    "lint": "eslint  --ext .ts,.js,.jsx",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*ts": [
      "yarn lint"
    ]
  },
  "private": true,
  "dependencies": {
    "@nestjs/cli": "8.2.5",
    "@nestjs/common": "8.0.0",
    "@nestjs/core": "8.0.0",
    "@nestjs/platform-express": "8.0.0",
    "@prisma/client": "4.1.0",
    "@supabase/supabase-js": "1.29.4",
    "@tokilabs/lang": "1.0.3",
    "@types/axios": "0.14.0",
    "@types/cron": "2.0.0",
    "@types/nodemailer-mailgun-transport": "1.4.3",
    "@types/url-join": "4.0.1",
