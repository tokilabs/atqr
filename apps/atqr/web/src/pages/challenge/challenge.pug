doctype html
head
  meta(charset='utf-8')
  meta(http-equiv='x-ua-compatible', content='ie=edge')
  title  ATQR - AJOELHOU TEM QUE REZAR
  meta(name='robots', content='noindex, follow')
  meta(name='description', content='')
  meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
  include ../index/_cdns.pug
  +headImports
.main-wrapper
  // Preloader start
  #preloader
    .preloader
      span
      span
  // Preloader End
  // Header Start
  .meeta-header-section.meeta-header-2
    // Header Middle Start
    .header-middle.header-sticky
      .container
        .header-wrap
          // Header Toggle Start
          .header-toggle.d-lg-none
            button(data-bs-toggle='offcanvas', data-bs-target='#offcanvasExample')
              span
              span
              span
          // Header Toggle End
        // Header Meta End
  // Header Middle End
// Register & Countdown Section Start
.meeta-register-countdown.register-countdown-4
  img.count-shape-3(src='../../assets/images/shape/counter-shape-1.png', alt='')
  .container.position-relative
    img.count-shape-1(src='../../assets/images/shape/count-shape-1.png', alt='')
    img.count-shape-2(src='../../assets/images/shape/count-shape-2.png', alt='')
    // Register & Countdown Wrapper Start
    .meeta-register-countdown-wrapper
      .row.gy-5.align-items-center
        .col-lg-12
          // Countdown Start
          .meeta-countdown.countdown(data-countdown='2023/03/23', data-format='short')
            .single-countdown
              span.count.countdown__time.daysLeft
              span.value.countdown__time.daysText
            .single-countdown
              span.count.countdown__time.hoursLeft
              span.value.countdown__time.hoursText
            .single-countdown
              span.count.countdown__time.minsLeft
              span.value.countdown__time.minsText
            .single-countdown
              span.count.countdown__time.secsLeft
              span.value.countdown__time.secsText
          // Countdown End
      // Register & Countdown Wrapper End
// Register & Countdown Section End
// About Section Start
.meeta-about-section-4.section-padding
  img.shape-1(src='../../assets/images/shape/counter-shape-2.png', alt='')
  img.shape-2(src='../../assets/images/shape/hero-shape-2.png', alt='')
  img.shape-3(src='../../assets/images/shape/ab-shape-4.png', alt='')
  .container
    .row
      .col-lg-6
        // About Images Start
        .meeta-about-counter-wrap
          .single-about-counter.text-center
            span.value 20
            p.text Speakers
          .single-about-counter.text-center
            span.value 05
            p.text Workshop
          .single-about-counter.text-center
            span.value 1140
            p.text Attendance
        // About Images End
      .col-lg-6
        .meeta-about-content-wrap
          // Section Title Start
          .meeta-section-title-2.section-title-4
            h2.main-title Seu desafio ${} foi criado!
          // Section Title End
          // About Content Start
          .meeta-about-content-4
            p
              | We&rsquo;re inviting the top creatives in the tech industry from all over the world to come learn, grow, scrape their knees, try new things, to be vulnerable, and to have epic adventures together, this time both in-person
            .about-list
              ul
                li.about-list-item
                  .about-icon
                    img(src='../../assets/images/ab-icon-5.png', alt='')
                  .about-text
                    h3.title Where is location
                    p Fort Mason Center San Francisco, Boston
                li.about-list-item
                  .about-icon
                    img(src='../../assets/images/ab-icon-6.png', alt='')
                  .about-text
                    h3.title When Will Start
                    p Sunday to Wednesday July 23 to 26, 2022
          // About Content End
// About Section End
// back to top start
.progress-wrap
  svg.progress-circle.svg-content(width='100%', height='100%', viewbox='-1 -1 102 102')
    path(d='M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98')
// back to top end
+scriptImports
