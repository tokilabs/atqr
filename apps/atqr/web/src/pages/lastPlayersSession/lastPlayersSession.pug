.meeta-topics-section.section-padding
  .container
    .meeta-topics-wrap
      // Section Title Start
      .meeta-section-title-2.section-title-3.text-center
        h4.sub-title
        h2.main-title &Uacute;ltimos desafiantes
      // Section Title End
      .meeta-topics-content-wrap.meeta-topics-active.slider-bullet
        .swiper.swiper-initialized.swiper-horizontal.swiper-pointer-events
          #swiper-wrapper-aea17340d60861eb.swiper-wrapper(aria-live='polite' style='transform: translate3d(-1638px, 0px, 0px); transition-duration: 0ms;')
            .swiper-slide.swiper-slide-duplicate.swiper-slide-duplicate-active(role='group' aria-label='3 / 3' style='width: 516px; margin-right: 30px;' data-swiper-slide-index='2')
              // Topics Item Start
              .topic-item
                .topic-img
                .topic-content(style='background-image: url(/assets/images/shape/topics-shape.png);')
                  h3.title
              // Topics Item End
            .swiper-slide.swiper-slide-duplicate-next(role='group' aria-label='1 / 3' style='width: 516px; margin-right: 30px;' data-swiper-slide-index='0')
              // Topics Item Start
              .topic-item
                .topic-img
                .topic-content(style='background-image: url(/assets/images/shape/topics-shape.png);')
                  h3.title
              // Topics Item End
            .swiper-slide.swiper-slide-prev(role='group' aria-label='2 / 3' style='width: 516px; margin-right: 30px;' data-swiper-slide-index='1')
              // Topics Item Start
              .topic-item
                .topic-img
                .topic-content(style='background-image: url(/assets/images/shape/topics-shape.png);')
                  h3.title
              // Topics Item End
              // Add Pagination
              .swiper-pagination.swiper-pagination-clickable.swiper-pagination-bullets.swiper-pagination-horizontal
                span.swiper-pagination-bullet(tabindex='0' role='button' aria-label='Go to slide 1')
                span.swiper-pagination-bullet(tabindex='0' role='button' aria-label='Go to slide 2')
                span.swiper-pagination-bullet.swiper-pagination-bullet-active(tabindex='0' role='button' aria-label='Go to slide 3')
              span.swiper-notification(aria-live='assertive' aria-atomic='true')
